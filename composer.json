{
  "name": "Osedea/zf2-osd-encrypt",
  "description": "A module for encrypting entities with zf2 and doctrine.",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Osedea/zf2-osd-encrypt.git"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "zendframework/zend-loader": "~2.1",
    "zendframework/zend-modulemanager": "~2.1",
    "zendframework/zend-mvc": "~2.1",
    "zendframework/zend-servicemanager": "~2.1",
    "zendframework/zend-stdlib": "~2.1",
    "zendframework/zend-eventmanager": "~2.1"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-0": {
      "OsdEncrypt": "src/"
    },
    "classmap": [
      "./Module.php"
    ]
  }
}
