{
    "name": "damacisaac/zf2-osd-encrypt",
    "description": "A module for encrypting entities with zf2 and doctrine.",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/damacisaac/zf2-osd-encrypt.git"
        }
    ],
        "require": {
        "php": ">=5.3.3",
        "zendframework/zend-crypt": "~2.1",
        "zendframework/zend-inputfilter": "~2.1",
        "zendframework/zend-servicemanager": "~2.1",
        "zendframework/zend-validator": "~2.1",
        "doctrine/doctrine-orm-module": "0.*"
    },
    "autoload": {
        "psr-0" : {
            "OsdEncrypt" : "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
